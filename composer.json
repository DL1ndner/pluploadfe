{
	"name": "felixnagel/pluploadfe",
	"type": "typo3-cms-extension",
	"description": "Provides an API and a FE plugin for using plupload, a highly usable and advanced upload handler.",
	"keywords": [
		"TYPO3 CMS",
		"upload",
		"pluploadfe"
	],
	"homepage": "https://github.com/fnagel/pluploadfe",
	"support": {
		"issues": "https://github.com/fnagel/pluploadfe/issues"
	},
	"authors": [
		{
			"name": "Felix Nagel",
			"email": "info@felixnagel.com",
			"homepage": "http://www.felixnagel.com",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0+",
	"require": {
		"php": ">=5.6,<7.2",
		"ext-json": "*",
		"typo3/cms-core": ">=7.6.11,<=8.7.99"
	},
	"suggest": {
		"typo3-ter/mailfiles": "Simple message form with file upload",
		"ext-fileinfo": "*"
	},
	"autoload": {
		"psr-4": {
			"FelixNagel\\Pluploadfe\\": "Classes/"
		}
	}
}
